{
  "name": "vorta",
  "version": "1.0.0",
  "description": "a tool to manage your local",
  "main": "dist/index.js",
  "repository": "git@github.com:hweeks/vorta.git",
  "author": "hammy-late-nite <git@hweeks.com>",
  "license": "MIT",
  "bin": {
    "vorta": "dist/index.js"
  },
  "scripts": {
    "lint": "eslint ./ --ext .js,.ts",
    "test": "jest",
    "test:debug": "node debug.js",
    "test:debug:nix": "node --inspect-brk ./node_modules/.bin/jest --runInBand --no-cache --watchAll",
    "test:debug:win": "node --inspect-brk ./node_modules/jest-cli/bin/jest.js --runInBand --no-cache --watchAll",
    "dev": "nodemon -e ts --exec yarn bb",
    "dev:brk": "nodemon -e ts --exec yarn bbb",
    "bbb": "babel-node --inspect-brk --extensions \".js,.ts\"",
    "bb": "babel-node --inspect --extensions \".js,.ts\"",
    "prepublishOnly": "tsc"
  },
  "devDependencies": {
    "@babel/cli": "7.13.14",
    "@babel/core": "7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "7.13.15",
    "@babel/preset-typescript": "7.13.0",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/npm": "7.0.8",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/jest": "26.0.22",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "14.14.37",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "commitizen": "4.2.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.24.0",
    "jest": "26.6.3",
    "nodemon": "^2.0.7",
    "semantic-release": "17.2.4",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "yargs": "^16.2.0"
  }
}
